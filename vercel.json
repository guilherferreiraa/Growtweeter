{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/user",
      "methods": ["GET", "POST", "DELETE"],
      "dest": "src/index.ts"
    },
    {
      "src": "/tweet",
      "methods": ["GET", "POST", "DELETE"],
      "dest": "src/index.ts"
    },
    {
      "src": "/tweet/(.*)",
      "methods": ["GET", "POST", "DELETE"],
      "dest": "src/index.ts"
    },
    {
      "src": "/",
      "dest": "src/index.ts"
    }
  ]
}